{% extends "base.html.twig" %}

{% block body %}
    <h2>Inscrivez-vous !</h2>
    {{ form_start(form) }}
    {{ form_row(form.firstname)}}
    {{ form_row(form.lastname)}}
    {{ form_row(form.email)}}
    {{ form_row(form.password)}}
    <button type="submit">S'inscrire</button>
    {{ form_end(form)}}

{% endblock %}